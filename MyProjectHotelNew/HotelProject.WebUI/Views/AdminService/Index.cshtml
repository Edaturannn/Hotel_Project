@model List<ResultServiceDto>
@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Index";
    int count = 0;
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Icon</th>
        <th>Title</th>
        <th>Description</th>
        <th>Güncelle</th>
        <th>Sil</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
            count++;
        <tr>
            <th>@count</th>
            <td>@item.ServiceIcon</td>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td><a href="/AdminService/UpdateService/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-warning">Güncelle</a></td>
            <td><a href="/AdminService/DeleteService/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>
<td><a href="/AdminService/AddService" class="btn mb-1 btn-rounded btn-outline-danger">Ekle</a></td>
