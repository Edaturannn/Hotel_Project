@model List<ResultStaffDto>
@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Index";
    int count = 0;
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Personel Adı</th>
        <th>Personel Ünvan</th>
        <th>Güncelle</th>
        <th>Sil</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
            count++;
        <tr>
            <th>@count</th>
            <td>@item.Name</td>
            <td>@item.Title</td>
            <td><a href="/AdminStaff/UpdateStaff/@item.StaffID" class="btn mb-1 btn-rounded btn-outline-warning">Güncelle</a></td>
            <td><a href="/AdminStaff/DeleteStaff/@item.StaffID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>
<td><a href="/AdminStaff/AddStaff" class="btn mb-1 btn-rounded btn-outline-info">Ekle</a></td>