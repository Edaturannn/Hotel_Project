@model List<ResultSubsribeDto>
@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Index";
    int count = 0;
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Mail</th>
        <th>Güncelle</th>
        <th>Sil</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
        {
            count++;
        <tr>
            <th>@count</th>
            <td>@item.Mail</td>
            <td><a href="/AdminSubsribe/UpdateSubsribe/@item.SubsribeID" class="btn mb-1 btn-rounded btn-outline-warning">Güncelle</a></td>
            <td><a href="/AdminSubsribe/DeleteSubsribe/@item.SubsribeID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>
<td><a href="/AdminSubsribe/AddSubsribe" class="btn mb-1 btn-rounded btn-outline-info">Ekle</a></td>
